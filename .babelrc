{
  "presets": ["eslatest-node6"],
  "plugins": [
    "syntax-async-functions",
    "transform-regenerator",
    "add-module-exports",
    "transform-runtime",
    "transform-es2015-destructuring",
    "transform-es2015-parameters",
    ["transform-object-rest-spread", { "useBuiltIns": true }]
  ]
}
